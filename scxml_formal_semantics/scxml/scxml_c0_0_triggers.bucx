/*
 * This context declares the set of triggers
 * @author cfsnook
 */
context scxml_c0_0_triggers
sets
	TRIGGERS // The set of triggers
constants
	InternalTriggers ExternalTriggers
	nullTrigger
axioms
	@triggerKinds: partition (TRIGGERS, InternalTriggers, ExternalTriggers, {nullTrigger})
	theorem @nte: nullTrigger ∉ ExternalTriggers
	theorem @nti: nullTrigger ∉ InternalTriggers
end